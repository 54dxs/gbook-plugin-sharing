{
  "name": "gbook-plugin-sharing",
  "description": "GBook网站分享插件",
  "main": "index.js",
  "version": "1.0.1",
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "engines": {
    "gbook": ">=1.0.0"
  },
  "homepage": "https://github.com/54dxs/gbook-plugin-sharing",
  "repository": {
    "type": "git",
    "url": "https://github.com/54dxs/gbook-plugin-sharing.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/54dxs/gbook-plugin-sharing/issues"
  },
  "gbook": {
    "properties": {
      "all": {
        "default": [
          "douban",
          "facebook",
          "twitter",
          "instapaper",
          "linkedin"
        ],
        "items": {
          "type": "string"
        },
        "type": "array",
        "uniqueItems": true
      },
      "qq": {
        "default": true,
        "description": "QQ",
        "type": "boolean"
      },
      "qzone": {
        "default": true,
        "description": "QQ空间",
        "type": "boolean"
      },
      "weibo": {
        "default": true,
        "description": "新浪微博",
        "type": "boolean"
      },
      "douban": {
        "default": false,
        "title": "豆瓣",
        "type": "boolean"
      },
      "facebook": {
        "default": false,
        "title": "Facebook",
        "type": "boolean"
      },
      "google": {
        "default": false,
        "title": "Google",
        "type": "boolean"
      },
      "hatenaBookmark": {
        "default": false,
        "description": "はてなブックマーク",
        "type": "boolean"
      },
      "instapaper": {
        "default": false,
        "description": "Instapaper",
        "type": "boolean"
      },
      "line": {
        "default": false,
        "description": "LINE",
        "type": "boolean"
      },
      "linkedin": {
        "default": false,
        "description": "Linkedin",
        "type": "boolean"
      },
      "messenger": {
        "default": false,
        "description": "Facebook Messenger",
        "type": "boolean"
      },
      "pocket": {
        "default": false,
        "description": "Pocket",
        "type": "boolean"
      },
      "stumbleupon": {
        "default": false,
        "description": "StumbleUpon",
        "type": "boolean"
      },
      "twitter": {
        "default": false,
        "title": "Twitter",
        "type": "boolean"
      },
      "viber": {
        "default": false,
        "description": "Viber",
        "type": "boolean"
      },
      "vk": {
        "default": false,
        "description": "VK",
        "type": "boolean"
      },
      "whatsapp": {
        "default": false,
        "description": "WhatsApp",
        "type": "boolean"
      }
    }
  },
  "devDependencies": {
    "eslint": "^2.7.0"
  }
}
